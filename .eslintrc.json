{
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"env": {
		"react-native/react-native": true,
		"jest": true,
		"browser": true
	},
	"extends": ["eslint:recommended"],
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": ["react", "react-native"],
	"rules": {
		"react/jsx-uses-react": [0],
		"no-unused-vars": "off"
	}
}
